RewriteEngine on
DirectoryIndex index.php
RewriteBase /mailbot/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^([a-zA-Z-_0-9]+)/([a-zA-Z-_0-9]+)/([a-zA-Z-_0-9]+)/?$ index.php?page1=$1&page2=$2&page3=$3 [L]
RewriteRule ^([a-zA-Z-_0-9]+)/([a-zA-Z-_0-9]+)/?$ index.php?page1=$1&page2=$2 [L]
RewriteRule ^([a-zA-Z-_0-9]+)/?$ index.php?page1=$1 [L]
